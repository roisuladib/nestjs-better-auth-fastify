{
	"name": "nestjs-better-auth-fastify",
	"description": "NestJS Better Auth Module for fastify adapter",
	"version": "0.0.8",
	"files": [
		"dist"
	],
	"module": "./dist/index.mjs",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "bunup && tsc --emitDeclarationOnly",
		"dev": "bunup --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"tsc": "tsc --noEmit",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"release": "bumpp --commit --push --tag",
		"prepare": "bun simple-git-hooks"
	},
	"dependencies": {
		"better-auth": "^1.3.26"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@nestjs/graphql": "^13.2.0",
		"@types/bun": "^1.2.23",
		"bumpp": "^10.2.3",
		"bunup": "^0.14.16",
		"simple-git-hooks": "^2.13.1",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"@nestjs/common": "^11.1.6",
		"@nestjs/core": "^11.1.6",
		"@nestjs/graphql": "^13.2.0",
		"@nestjs/platform-fastify": "^11.1.6",
		"fastify": "^5.6.1",
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"@nestjs/core": {
			"optional": false
		},
		"@nestjs/common": {
			"optional": false
		},
		"@nestjs/graphql": {
			"optional": true
		},
		"@nestjs/platform-fastify": {
			"optional": false
		},
		"better-auth": {
			"optional": false
		},
		"fastify": {
			"optional": false
		},
		"typescript": {
			"optional": true
		}
	},
	"simple-git-hooks": {
		"pre-commit": "bun run lint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/roisuladib/nestjs-better-auth-fastify.git"
	},
	"homepage": "https://github.com/roisuladib/nestjs-better-auth-fastify#readme",
	"bugs": {
		"url": "https://github.com/roisuladib/nestjs-better-auth-fastify/issues"
	},
	"license": "MIT"
}
