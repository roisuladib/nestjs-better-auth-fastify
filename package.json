{
	"name": "nestjs-better-auth-fastify",
	"description": "NestJS Better Auth Module for fastify adapter",
	"version": "0.0.7",
	"files": [
		"dist"
	],
	"module": "./dist/index.mjs",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "bunup && tsc --emitDeclarationOnly --declaration",
		"dev": "bunup --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"tsc": "tsc --noEmit",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"release": "bumpp --commit --push --tag",
		"publish:ci": "bun publish --access public --no-git-checks",
		"prepare": "simple-git-hooks"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.2",
		"@types/bun": "^1.2.21",
		"bumpp": "^10.2.3",
		"bunup": "^0.11.23",
		"simple-git-hooks": "^2.13.1",
		"typescript": "^5.9.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/roisuladib/nestjs-better-auth-fastify.git"
	},
	"peerDependencies": {
		"@nestjs/common": "^11.1.6",
		"@nestjs/core": "^11.1.6",
		"@nestjs/platform-fastify": "^11.1.6",
		"fastify": "^5.5.0",
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"@nestjs/core": {
			"optional": false
		},
		"@nestjs/common": {
			"optional": false
		},
		"@nestjs/platform-fastify": {
			"optional": false
		},
		"better-auth": {
			"optional": false
		},
		"fastify": {
			"optional": false
		},
		"typescript": {
			"optional": true
		}
	},
	"simple-git-hooks": {
		"pre-commit": "bun run lint:fix && git add ."
	},
	"homepage": "https://github.com/roisuladib/nestjs-better-auth-fastify#readme",
	"bugs": {
		"url": "https://github.com/roisuladib/nestjs-better-auth-fastify/issues"
	},
	"license": "MIT",
	"dependencies": {
		"better-auth": "^1.3.8"
	}
}
